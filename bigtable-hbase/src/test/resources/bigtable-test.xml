<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>
    <property>
        <name>hbase.client.connection.impl</name>
        <value>org.apache.hadoop.hbase.client.BigtableConnection</value>
    </property>
    <property>
        <name>google.bigtable.endpoint.host</name>
        <value>${google.bigtable.endpoint.host}</value>
    </property>
    <property>
        <name>google.bigtable.endpoint.port</name>
        <value>443</value>
    </property>
    <property>
        <name>google.bigtable.admin.endpoint.host</name>
        <value>${google.bigtable.admin.endpoint.host}</value>
    </property>
    <property>
        <name>google.bigtable.cluster.admin.endpoint.host</name>
        <value>${google.bigtable.cluster.admin.endpoint.host}</value>
    </property>
    <property>
        <name>google.bigtable.project.id</name>
        <value>autonomous-mote-782</value>
    </property>
    <property>
        <name>google.bigtable.zone.name</name>
        <value>us-central1-b</value>
    </property>
    <property>
        <name>google.bigtable.cluster.name</name>
        <value>cluster</value>
    </property>
    <property>
        <name>google.bigtable.auth.service.account.enable</name>
        <value>${google.bigtable.auth.service.account.enable}</value>
    </property>
    <property>
        <name>google.bigtable.auth.null.credential.enable</name>
        <value>true</value>
    </property>
    <property>
        <name>google.bigtable.call.report.directory</name>
        <value>${google.bigtable.call.report.directory}</value>
    </property>
    <property>
        <name>google.bigtable.auth.service.account.email</name>
        <value>${google.bigtable.auth.test.service.account.email}</value>
    </property>
    <property>
        <name>google.bigtable.auth.service.account.keyfile</name>
        <value>${google.bigtable.auth.test.service.account.keyfile}</value>
    </property>
</configuration>
